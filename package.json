{
  "name": "monadical",
  "version": "1.4.0",
  "description": "A set of monadic data types",
  "main": "src/index.js",
  "scripts": {
    "build": "scripts/build",
    "prepublish": "scripts/build",
    "test": "scripts/test"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "src/*.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "verbose": true,
    "compileEnhancements": true,
    "require": [
      "@babel/register"
    ],
    "babel": {
      "extensions": [
        "js"
      ]
    }
  },
  "author": "Antonio G. Greco",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agreco/monadical"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/register": "^7.4.0",
    "ava": "^1.3.1"
  }
}
